#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/tomis/Documents/QL_CTDT_FRONT/node_modules/.pnpm/@colyseus+schema@3.0.54/node_modules/@colyseus/schema/bin/node_modules:/mnt/c/Users/tomis/Documents/QL_CTDT_FRONT/node_modules/.pnpm/@colyseus+schema@3.0.54/node_modules/@colyseus/schema/node_modules:/mnt/c/Users/tomis/Documents/QL_CTDT_FRONT/node_modules/.pnpm/@colyseus+schema@3.0.54/node_modules/@colyseus/node_modules:/mnt/c/Users/tomis/Documents/QL_CTDT_FRONT/node_modules/.pnpm/@colyseus+schema@3.0.54/node_modules:/mnt/c/Users/tomis/Documents/QL_CTDT_FRONT/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/tomis/Documents/QL_CTDT_FRONT/node_modules/.pnpm/@colyseus+schema@3.0.54/node_modules/@colyseus/schema/bin/node_modules:/mnt/c/Users/tomis/Documents/QL_CTDT_FRONT/node_modules/.pnpm/@colyseus+schema@3.0.54/node_modules/@colyseus/schema/node_modules:/mnt/c/Users/tomis/Documents/QL_CTDT_FRONT/node_modules/.pnpm/@colyseus+schema@3.0.54/node_modules/@colyseus/node_modules:/mnt/c/Users/tomis/Documents/QL_CTDT_FRONT/node_modules/.pnpm/@colyseus+schema@3.0.54/node_modules:/mnt/c/Users/tomis/Documents/QL_CTDT_FRONT/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@colyseus/schema/bin/schema-debug" "$@"
else
  exec node  "$basedir/../@colyseus/schema/bin/schema-debug" "$@"
fi
